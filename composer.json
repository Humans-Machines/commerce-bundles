{
	"name": "webdna/commerce-bundles",
	"description": "Bundles plugin for Craft Commerce",
	"type": "craft-plugin",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"bundles"
	],
	"support": {
		"docs": "https://github.com/webdna/commerce-bundles",
		"issues": "https://github.com/webdna/commerce-bundles/issues?state=open"
	},
	"license": "proprietary",
	"authors": [
		{
			"name": "WebDNA",
			"homepage": "https://webdna.co.uk"
		}
	],
	"require": {
		"craftcms/cms": "^4.0",
		"craftcms/commerce": "^4.0"
	},
	"autoload": {
		"psr-4": {
			"webdna\\commerce\\bundles\\": "src/"
		}
	},
	"extra": {
		"name": "Commerce Bundles",
		"handle": "commerce-bundles",
		"class": "webdna\\commerce\\bundles\\Bundles"
	}
}
